<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context=".HomeActivity"
    android:background="@color/white">


    <ImageView
        android:id="@+id/home_detect_button"
        android:layout_width="359dp"
        android:layout_height="257dp"
        android:layout_marginTop="36dp"
        android:scaleType="fitXY"
        android:src="@drawable/mainbtn11"
        app:layout_constraintHorizontal_bias="0.403"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent">
    </ImageView>

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="25dp"
        android:src="@drawable/scan"
        app:layout_constraintEnd_toEndOf="@+id/home_detect_button"
        app:layout_constraintStart_toStartOf="@+id/home_detect_button"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/sd5"
        android:text="감지"
        android:textColor="@color/white"
        android:textSize="40sp"
        android:layout_marginTop="55dp"
        app:layout_constraintEnd_toEndOf="@+id/home_detect_button"
        app:layout_constraintStart_toStartOf="@+id/home_detect_button"
        app:layout_constraintTop_toTopOf="@+id/home_detect_button"
        app:layout_constraintVertical_bias="0.138" />

    <ImageView
        android:id="@+id/home_community_button"
        android:layout_width="175dp"
        android:layout_height="170dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="264dp"
        android:scaleType="fitXY"
        android:src="@drawable/mainbtn_left"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"></ImageView>

    <ImageView
        android:id="@+id/home_info_button"
        android:layout_width="175dp"
        android:layout_height="170dp"
        android:layout_marginTop="264dp"
        android:layout_marginRight="16dp"
        android:scaleType="fitXY"
        android:src="@drawable/mainbtn_right"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"></ImageView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/sd5"
        android:text="견종 정보"
        android:textColor="@color/white"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="@+id/home_info_button"
        app:layout_constraintEnd_toEndOf="@+id/home_info_button"
        app:layout_constraintHorizontal_bias="0.48"
        app:layout_constraintStart_toStartOf="@+id/home_info_button"
        app:layout_constraintTop_toTopOf="@+id/home_info_button"
        app:layout_constraintVertical_bias="0.838" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/sd5"
        android:text="커뮤니티"
        android:textColor="@color/white"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="@+id/home_community_button"
        app:layout_constraintEnd_toEndOf="@+id/home_community_button"
        app:layout_constraintHorizontal_bias="0.457"
        app:layout_constraintStart_toStartOf="@+id/home_community_button"
        app:layout_constraintTop_toTopOf="@+id/home_community_button"
        app:layout_constraintVertical_bias="0.838" />


    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:backgroundTint="@color/white"
        android:src="@drawable/community"
        app:layout_constraintBottom_toBottomOf="@+id/home_community_button"
        app:layout_constraintEnd_toEndOf="@+id/home_community_button"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="@+id/home_community_button"
        app:layout_constraintTop_toTopOf="@+id/home_community_button"
        app:layout_constraintVertical_bias="0.416" />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:backgroundTint="@color/white"
        android:src="@drawable/paper"
        app:layout_constraintBottom_toBottomOf="@+id/home_info_button"
        app:layout_constraintEnd_toEndOf="@+id/home_info_button"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="@+id/home_info_button"
        app:layout_constraintTop_toTopOf="@+id/home_info_button"
        app:layout_constraintVertical_bias="0.416" />

    <ImageView
        android:id="@+id/home_faq_button"
        android:layout_width="350dp"
        android:layout_height="100dp"
        android:layout_marginTop="448dp"
        android:src="@drawable/faq_btn"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginLeft="30dp"
        android:layout_marginBottom="6dp"
        android:src="@drawable/chat"
        app:layout_constraintBottom_toBottomOf="@+id/home_faq_button"
        app:layout_constraintStart_toStartOf="@+id/home_faq_button"
        app:layout_constraintTop_toTopOf="@+id/home_faq_button" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/sd5"
        android:text="FAQ"
        android:textColor="@color/white"
        android:textSize="28sp"
        app:layout_constraintBottom_toBottomOf="@+id/home_faq_button"
        android:layout_marginLeft="25dp"
        android:layout_marginBottom="6dp"
        app:layout_constraintStart_toEndOf="@+id/imageView6"
        app:layout_constraintTop_toTopOf="@+id/home_faq_button" />

    <ImageView
        android:id="@+id/home_faq_button22"
        android:layout_width="350dp"
        android:layout_height="100dp"
        android:layout_marginTop="548dp"
        android:src="@drawable/faq_btn"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView22"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginLeft="30dp"
        android:layout_marginBottom="6dp"
        android:src="@drawable/ic_baseline_gpp_maybe_24"
        app:layout_constraintBottom_toBottomOf="@+id/home_faq_button22"
        app:layout_constraintStart_toStartOf="@+id/home_faq_button22"
        app:layout_constraintTop_toTopOf="@+id/home_faq_button22" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginBottom="6dp"
        android:fontFamily="@font/sd5"
        android:text="신고 기준"
        android:textColor="@color/white"
        android:textSize="28sp"
        app:layout_constraintBottom_toBottomOf="@+id/home_faq_button22"
        app:layout_constraintStart_toEndOf="@+id/imageView22"
        app:layout_constraintTop_toTopOf="@+id/home_faq_button22"
        app:layout_constraintVertical_bias="0.526" />

</androidx.constraintlayout.widget.ConstraintLayout>
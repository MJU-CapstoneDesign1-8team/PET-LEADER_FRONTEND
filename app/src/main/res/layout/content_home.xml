<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context=".HomeActivity"
    android:background="@color/white">


    <ImageView
        android:id="@+id/home_detect_button"
        android:layout_width="359dp"
        android:layout_height="257dp"
        android:layout_marginTop="32dp"
        android:scaleType="fitXY"
        android:src="@drawable/mainbtn11"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent">


    </ImageView>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/sd6"
        android:text="감지"
        android:textColor="@color/white"
        android:textSize="40sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.138" />

    <ImageView
        android:id="@+id/home_community_button"
        android:layout_width="175dp"
        android:layout_height="170dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="264dp"
        android:scaleType="fitXY"
        android:src="@drawable/mainbtn_left"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"></ImageView>

    <ImageView
        android:id="@+id/home_info_button"
        android:layout_width="175dp"
        android:layout_height="170dp"
        android:layout_marginTop="264dp"
        android:layout_marginRight="24dp"
        android:scaleType="fitXY"
        android:src="@drawable/mainbtn_right"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"></ImageView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/sd5"
        android:text="견종 정보"
        android:textColor="@color/white"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="@+id/home_info_button"
        app:layout_constraintEnd_toEndOf="@+id/home_info_button"
        app:layout_constraintHorizontal_bias="0.48"
        app:layout_constraintStart_toStartOf="@+id/home_info_button"
        app:layout_constraintTop_toTopOf="@+id/home_info_button"
        app:layout_constraintVertical_bias="0.838" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/sd5"
        android:text="커뮤니티"
        android:textColor="@color/white"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="@+id/home_community_button"
        app:layout_constraintEnd_toEndOf="@+id/home_community_button"
        app:layout_constraintHorizontal_bias="0.457"
        app:layout_constraintStart_toStartOf="@+id/home_community_button"
        app:layout_constraintTop_toTopOf="@+id/home_community_button"
        app:layout_constraintVertical_bias="0.838" />


    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:backgroundTint="@color/white"
        android:src="@drawable/peoplewhite"
        app:layout_constraintBottom_toBottomOf="@+id/home_community_button"
        app:layout_constraintEnd_toEndOf="@+id/home_community_button"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="@+id/home_community_button"
        app:layout_constraintTop_toTopOf="@+id/home_community_button"
        app:layout_constraintVertical_bias="0.416" />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:backgroundTint="@color/white"
        android:src="@drawable/file_white12"
        app:layout_constraintBottom_toBottomOf="@+id/home_info_button"
        app:layout_constraintEnd_toEndOf="@+id/home_info_button"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="@+id/home_info_button"
        app:layout_constraintTop_toTopOf="@+id/home_info_button"
        app:layout_constraintVertical_bias="0.416" />

    <ImageView
        android:id="@+id/home_faq_button"
        android:layout_width="350dp"
        android:layout_height="100dp"
        android:layout_marginTop="448dp"
        android:src="@drawable/faq_button"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>